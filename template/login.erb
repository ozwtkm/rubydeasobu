<%
	result = {}

	if !c[:e].nil? 
		if c[:e].class == Error_multi_412
			error_list = c[:e].exceptions

			result = error_list.map do |e|
				{:ErrorMessage => e.message}
			end
		else
			result = {:ErrorMessage => c[:e].message}
		end


	end

	if !c[:user].nil?
		result = {
			:username=>c[:user].name
		}
	end
%><%= JSON.generate(result) %>