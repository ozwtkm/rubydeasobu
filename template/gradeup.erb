<%
	result = {}

	if !c[:e].nil? 
		result = {:ErrorMessage=>c[:e].message}
	end 

	if !c[:monster].nil?
		result = {
			:id=>c[:monster].id,
			:name=>c[:monster].name, 
			:rarity=>c[:monster].rarity
		}
	end

	if !c[:recipes].nil?
		result = c[:recipes].map do |k,v|
			{
				:id=>v.recipe_id,
				:name=>v.recipe_name
			}
		end
	end

%><%= JSON.generate(result) %>